{
  "name": "react-testsql",
  "version": "0.9.0",
  "private": true,
  "homepage": "https://joshualicense.github.io/react-testsql",
  "proxy": "http://localhost:3001/",
  "dependencies": {
    "bcrypt-nodejs": "^0.0.3",
    "body-parser": "^1.18.2",
    "connect-mongo": "^2.0.1",
    "crypto": "^1.0.1",
    "dotenv": "^6.0.0",
    "errorhandler": "^1.5.0",
    "express": "4.13.3",
    "express-session": "^1.15.6",
    "express-validator": "^5.0.3",
    "glob": "^7.1.2",
    "mongoose": "^5.0.10",
