{
  "name": "@react-testsql/client",
  "version": "0.12.0",
  "private": true,
  "homepage": ".",
  "proxy": "http://localhost:3001/",
  "dependencies": {
    "@material-ui/core": "^4.0.0",
    "@material-ui/icons": "^3.0.0",
    "classnames": "^2.2.6",
    "file-saver": "^2.0.2",
    "gh-pages": "^2.1.1",
    "lodash": "^4.17.12",
    "marked": "^1.2.0",
    "react": "^16.3.2",
    "react-ace": "^6.2.0",
    "react-dom": "^16.3.2",
    "react-router-dom": "^5.1.2",
    "react-scripts": "^3.0.0",
    "recharts": "^1.3.1",
    "sql.js": "^1.0.0",
    "typeface-roboto": "^0.0.54"
  },
  "scripts": {
    "predeploy": "cross-env REACT_APP_CLIENT_ONLY=true PUBLIC_URL=https://joshualicense.github.io/react-testsql/ yarn build && yarn copy-wasm",
    "deploy": "gh-pages -d build",
    "start": "react-scripts start",
    "poststart": "yarn copy-wasm",
    "copy-wasm": "copy \"..\\node_modules\\sql.js\\dist\\sql-wasm.wasm\" \"build/static/js/sql-wasm.wasm\"",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "devDependencies": {
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "react-test-renderer": "^16.4.0"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!src/registerServiceWorker.js",
      "!src/setupTests.js",
      "!src/index.js"
    ]
  },
  "browserslist": {
    "development": [
      "last 2 chrome versions",
      "last 2 firefox versions",
      "last 2 edge versions"
    ],
    "production": [
      ">0.25%",
      "not op_mini all",
      "ie 11"
    ]
  }
}
